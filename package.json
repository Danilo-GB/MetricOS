    "pinia": "^2.0.13",
    "simple-statistics": "^7.7.5",
    "vue": "2.6.14",
    "vue-apexcharts": "^1.6.2",
    "vue-github-buttons": "^3.1.0",
    "vue-grid-layout": "^2.3.12",
    "vue-i18n": "^8.27.1",
    "vue-router": "3.5.2"
