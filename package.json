{
  "name": "muse-vue-ant-design-dashboard",
  "version": "1.0.0",
  "private": true,
  "description": "A user-friendly, open source and beautiful dashboard based on Ant Design Vue.",
  "scripts": {
    "serve": "vue-cli-service serve --open",
